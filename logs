                    return;
                }

                // Mendapatkan data negara dan kota pengguna
                var userCountry = response.country_name;
                var userCity = response.city;

                // Kota yang ditargetkan
                var targetCities = ['Pontianak', 'Singkawang', 'Palangkaraya', 'Balikpapan', 'Bontang', 'Samarinda', 'Manado', 'Palu', 'Kendari', 'Sintang', 'Sanggau', 'Sampit', 'Katangan', 'Gunung Mas', 'Barito Timur', 'Banjarmasin', 'Banjar Baru', 'Martapura', 'Amuntai', 'Pelaihari', 'Tenggarong', 'Kutai Kartanegara', 'Mahulu', 'Tanjung Selor', 'Tarakan', 'Nunukan', 'Sambas', 'Balangan', 'Tanah Bumbu'];

                // Melakukan tindakan jika negara adalah Indonesia dan kota berada dalam daftar
                if (userCountry === 'Indonesia' && targetCities.includes(userCity)) {
                    console.log("Pengguna dari kota yang ditargetkan:", userCity);
                    fbq('track', 'PageView', {
                        country: 'Indonesia',
                        city: userCity
                    });
                } else {
                    console.log("Pengguna tidak berada di kota yang ditargetkan.");
                }
            }).fail(function() {
                console.error("Gagal memuat data dari ipstack.");
            });
        });
    </script>
</body>
</html>
